<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="ZTurnPlatform"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
ZTurnPlatform
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="ZTurnPlatform">
        <sdx:configuration sdx:name="ZTurnPlatform"
                           sdx:displayName="ZTurnPlatform"
                           sdx:defaultProcessorGroup="core1"
                           sdx:runtimes="cpp">
            <sdx:description>ZTurnPlatform</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="ZTurnPlatform/boot/ZTurnPlatform.bif"
                            sdx:readme="ZTurnPlatform/boot/generic.readme"
                            sdx:qemuArguments="ZTurnPlatform/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="core0"
                            sdx:bif="ZTurnPlatform/boot/ZTurnPlatform.bif"
                            sdx:readme="ZTurnPlatform/boot/generic.readme"
                            sdx:qemuArguments="ZTurnPlatform/core0/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="core1"
                            sdx:bif="ZTurnPlatform/boot/ZTurnPlatform.bif"
                            sdx:readme="ZTurnPlatform/boot/generic.readme"
                            sdx:qemuArguments="ZTurnPlatform/core1/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="core0"
                                sdx:displayName="core0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="ZTurnPlatform/core0/bsplib/lib"
                        sdx:bspIncludePaths="ZTurnPlatform/core0/bspinclude/include"
                        sdx:bspConfig="ZTurnPlatform/core0/system.mss"
                />
            </sdx:processorGroup>
            <sdx:processorGroup sdx:name="core1"
                                sdx:displayName="core1"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_1">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="ZTurnPlatform/core1/bsplib/lib"
                        sdx:bspIncludePaths="ZTurnPlatform/core1/bspinclude/include"
                        sdx:bspConfig="ZTurnPlatform/core1/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
